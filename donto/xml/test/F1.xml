<?xml version="1.0" encoding="UTF-8"?>
<Root>

	<!-- Additive -->
	<A A1="1">
	    <B B1="2"/>
	    <B B1="3"/>
	</A>

	<!-- Greater -->
	<C C1="1" C2="2" C3="3">
	    <D D1="2"/>
	    <D D1="3"/>
	</C>

	<!-- LESS -->
	<E E1="1" E2="2" E3="3">
	    <F F1="2"/>
	    <F F1="3"/>
	</E>

	<!-- EQUALS -->
	<G G1="1" G2="2">
	    <H H1="2"/>
	    <H H1="3"/>
	</G>

</Root>
